{
  "compilerOptions": {
    "esModuleInterop": true,
    "declaration": true,
    "lib": ["es6", "dom", "dom.iterable"],
    "module": "es6",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "outDir": "./dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "paths": {
    "zlibjs/bin/zlib_and_gzip.min.js": [ "node_modules/zlibjs/bin/zlib_and_gzip.min.js" ]
  }
}